---
const { skill } = Astro.props;
const { frameworks } = Astro.props;
const { description } = Astro.props;
const { iconname } = Astro.props
import { Icon } from 'astro-icon'
---
<li class="bg-amber-200 h-auto w-auto rounded-3xl">
     <div><Icon name={ iconname } class="h-16 w-16"/></div>
     <h2>{ skill }</h2>
     <h4>{ frameworks }</h4>
     <span>{ description }</span>
</li>

